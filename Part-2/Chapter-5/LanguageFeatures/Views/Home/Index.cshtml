@model IEnumerable<string> 
@{
    Layout = null;
}
<!DOCTYPE html>
<html>
    <head>
        <title>
            Language Features
        </title>
        <meta name="viewport" content="width=device-width" />
    </head>
    <body>
        <ul>
        @foreach(string item in @Model)
        {
            <li>
                @item
            </li>
        }
        </ul>
    </body>
</html>