@model ListViewProducts

@foreach(Product item in Model.Products ?? Enumerable.Empty<Product>())
{
    <partial name="ProductSummary" model="item"/>

}

<div 
    page-model="@Model.pageModel" 
    page-action="Index" 
    css-page-normal="btn btn-primary m-1"
    class = "btn-group pull-right m-1"
    page-url-category ="@Model.Category"
    css-page-selected="btn btn-outline-dark m-1">
    
</div>
